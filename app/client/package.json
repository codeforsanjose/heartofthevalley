{
  "name": "heartofvalley-client",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "axios": "^1.6.2",
    "bootstrap": "^5.3.2",
    "jquery": "^3.7.1",
    "mapbox-gl": "^3.0.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-map-gl": "^7.1.6",
    "react-router-dom": "^6.20.1",
    "web-vitals": "^3.5.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test --watchAll=false",
    "cover": "npm run test -- --coverage",
    "lint": "eslint ./src",
    "format": "prettier --write \"{,!(node_modules)/**/}*.js\"",
    "watch": "npm-watch",
    "validate": "npm-run-all format lint test",
    "precommit": "npm run validate",
    "prepush": "npm run validate"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "proxy": "http://localhost:3000",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "prettier": "^3.1.0",
    "prop-types": "^15.8.1",
    "react-scripts": "5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "watch": {
    "build": "src/"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}
